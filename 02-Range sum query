#include<iostream>
using namespace std;

#define MAX 100000
int array[MAX]={0};
int bit[MAX]={0};

void update(int index,int value,int n){
    while(index<=n){
        bit[index]=bit[index]+value;
        index=index+(index&-index);
    }
}


int getsum(int bit[],int index){
    int sum=0;
    while(index>0){
        sum=sum+bit[index];
        index=index-(index&-index);

    }
    return sum;
}

int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>array[i];
    }
    for(int i=0;i<n;i++){
        update(i+1,array[i],n);
    }
    int r;
    cin>>r;

    cout<<getsum(bit,r+1)<<endl;


    return 0;
}