#include<iostream>
using namespace std;

#define MAX 100000
int array[MAX]={0};
int bit[MAX+1]={0};
//logic for update function
void update(int index,int value,int n){
    while(index<=n){
        bit[index]=bit[index]+value;
        index=index+(index&-index);
    }
    

}
int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    for(int i=0;i<n;i++){
        update(i+1,array[i],n);
    }

    for(int i=1;i<=n;i++){
        cout<<bit[i]<<endl;
    }


    return 0;
}

